<?xml version="1.0" encoding="UTF-8"?>
<templates>
    <t t-name="stock_3d_custom_view.ListView.Buttons" t-inherit="web.ListView.Buttons" t-inherit-mode="primary" owl="1">
        <xpath expr="//div[hasclass('o_list_buttons')]" position="inside">
            <button class="btn btn-primary" t-on-click="(ev) => this.open3DView(ev)">Preview Layout</button>
        </xpath>
    </t>
</templates>
